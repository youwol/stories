{
    "name": "@youwol/stories",
    "description": "YouWol Stories application",
    "version": "0.2.3-wip",
    "author": "greinisch@youwol.com",
    "homepage": "https://github.com/youwol/stories#README.md",
    "main": "dist/index.html",
    "types": "src/index.ts",
    "scripts": {
        "clean": "del-cli dist",
        "auto-gen": "python template.py",
        "build": "yarn build:dev",
        "pre-build": "yarn clean && yarn auto-gen",
        "build:dev": "yarn pre-build && webpack --mode development",
        "build:prod": "yarn pre-build && webpack --mode production",
        "lint-check": "yarn lint-prettier-check && yarn lint-eslint-check",
        "lint-prettier-check": "prettier --check ./src",
        "lint-eslint-check": "eslint ./src",
        "test": "jest -c ./jest.config.js -i --rootDir .",
        "test-coverage": "jest -c ./jest.config.js -i --rootDir . --collect-coverage --collectCoverageFrom=./src/lib/**/*.ts ",
        "doc": "typedoc",
        "publish-npm-public": "yarn build:prod && yarn test-coverage && yarn publish --access public",
        "start": "webpack serve"
    },
    "devDependencies": {
        "@types/node": "18.0.6",
        "typescript": "^4.7.4",
        "ts-lib": "^0.0.5",
        "ts-node": "^10.9.1",
        "ts-loader": "^9.3.1",
        "@types/jest": "27.4.1",
        "ts-jest": "27.1.3",
        "jest": "27.5.1",
        "jest-junit": "13.0.0",
        "typedoc": "^0.23.8",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.9.1",
        "webpack-bundle-analyzer": "^4.5.0",
        "@types/webpack": "^5.28.0",
        "del-cli": "^5.0.0",
        "@youwol/prettier-config": "^0.0.1",
        "@youwol/eslint-config": "^0.0.1",
        "prettier": "^2.7.1",
        "prettier-plugin-properties": "^0.2.0",
        "@typescript-eslint/eslint-plugin": "^5.33.1",
        "@typescript-eslint/parser": "^5.33.1",
        "eslint": "8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.8.4",
        "eslint-plugin-tsdoc": "^0.2.16",
        "eslint-plugin-unused-imports": "^2.0.0",
        "css-loader": "5.2.0",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.2.0",
        "mini-css-extract-plugin": "1.3.9",
        "source-map-loader": "2.0.1",
        "webpack-dev-server": "^4.7.1"
    },
    "prettier": "@youwol/prettier-config",
    "eslintConfig": {
        "extends": [
            "@youwol"
        ]
    },
    "dependencies": {
        "@youwol/os-core": "^0.1.1",
        "@youwol/fv-tree": "^0.2.3",
        "@youwol/os-top-banner": "^0.1.1",
        "@youwol/cdn-client": "^1.0.2",
        "@youwol/http-clients": "^1.0.2",
        "@youwol/flux-view": "^1.0.3",
        "@youwol/fv-context-menu": "^0.1.1",
        "rxjs": "^6.5.5",
        "grapesjs": "0.18.3",
        "codemirror": "^5.52.0"
    },
    "youwol": {
        "cdnDependencies": {
            "@youwol/os-core": "^0.1.1",
            "@youwol/fv-tree": "^0.2.3",
            "@youwol/os-top-banner": "^0.1.1",
            "@youwol/cdn-client": "^1.0.2",
            "@youwol/http-clients": "^1.0.2",
            "@youwol/flux-view": "^1.0.3",
            "@youwol/fv-context-menu": "^0.1.1",
            "rxjs": "^6.5.5",
            "grapesjs": "0.18.3"
        }
    }
}
